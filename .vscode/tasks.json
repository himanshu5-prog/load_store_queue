                "${workspaceFolder}/src/Load_Store_Queue/common_queue.cc",
                "${workspaceFolder}/src/system/system.cc",
                "${workspaceFolder}/src/mainFunc.cpp",
                "-std=c++14",
                "-o",
                "${workspaceFolder}/lsq"
